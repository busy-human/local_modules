    "bundled",
    "embed"
  ],
  "author": "Andi Neck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glintapp/local_modules/issues"
  },
  "homepage": "https://github.com/glintapp/local_modules#readme",
  "dependencies": {
    "defaults": "^1.0.3",
    "exe": "^1.0.2",
    "lodash.merge": "^4.6.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.4.3",
    "subarg": "^1.0.0",
    "typescript": "^4.6.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/yargs": "^17.0.9",
    "chai": "^4.3.6",
    "fs-extra": "^10.0.1",
    "mocha": "^9.2.1",
    "mocha-sinon": "^2.1.2"
  }
