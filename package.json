{
  "name": "@busy-human/local_modules",
  "version": "2.1.1",
  "description": "embeddable npm local_modules.",
  "main": "index.js",
  "bin": {
    "lpm": "dist/index.js",
    "local_modules": "dist/index.js"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "mocha --exclude **/test.old.js",
